!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_OUTPUT_MODE	u-ctags	/u-ctags or e-ctags/
!_TAG_PROGRAM_AUTHOR	Universal Ctags Team	//
!_TAG_PROGRAM_NAME	Universal Ctags	/Derived from Exuberant Ctags/
!_TAG_PROGRAM_URL	https://ctags.io/	/official site/
!_TAG_PROGRAM_VERSION	0.0.0	//
$(EXECUTABLE)	Makefile	/^$(EXECUTABLE): $(OBJECTS)$/;"	t
%.o	Makefile	/^%.o : %.c$/;"	t
BINDIR	Makefile	/^BINDIR=\/usr\/bin$/;"	m
CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS= -Wall$/;"	m
DESTDIR	Makefile	/^DESTDIR=$/;"	m
EXECUTABLE	Makefile	/^EXECUTABLE=hello$/;"	m
LDFLAGS	Makefile	/^LDFLAGS=$/;"	m
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.c=.o)$/;"	m
RM	Makefile	/^RM=rm$/;"	m
SOURCES	Makefile	/^SOURCES=hello.c$/;"	m
all	Makefile	/^all: $(SOURCES) $(EXECUTABLE)$/;"	t
clean	Makefile	/^clean:$/;"	t
install	Makefile	/^install: $(EXECUTABLE)$/;"	t
main	hello.c	/^int main(int argc, char **argv)$/;"	f	typeref:typename:int
