!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_OUTPUT_MODE	u-ctags	/u-ctags or e-ctags/
!_TAG_PROGRAM_AUTHOR	Universal Ctags Team	//
!_TAG_PROGRAM_NAME	Universal Ctags	/Derived from Exuberant Ctags/
!_TAG_PROGRAM_URL	https://ctags.io/	/official site/
!_TAG_PROGRAM_VERSION	0.0.0	//
%.o	Makefile	/^%.o : %.c$/;"	t
CC	Makefile	/^CC = gcc$/;"	m
DIR	Makefile	/^DIR = $(abspath .\/)$/;"	m
INC	Makefile	/^INC = $(addprefix .\/,inc)$/;"	m
LIB	Makefile	/^LIB = $(OUTPUT)\/libbasic.so.1.0.0$/;"	m
OBJS	Makefile	/^OBJS = $(S_SRC:.c=.o)$/;"	m
OUTPUT	Makefile	/^OUTPUT = build$/;"	m
SRC	Makefile	/^SRC = $(addprefix .\/,src)$/;"	m
S_INC	Makefile	/^S_INC = $(wildcard $(INC)\/*.h)$/;"	m
S_SRC	Makefile	/^S_SRC = $(wildcard $(SRC)\/*.c)$/;"	m
all	Makefile	/^all: lib$/;"	t
clean	Makefile	/^clean:$/;"	t
goodbye	src/goodbye.c	/^void goodbye()$/;"	f	typeref:typename:void
hello	src/hello.c	/^void hello()$/;"	f	typeref:typename:void
lib	Makefile	/^lib:$(OBJS)$/;"	t
